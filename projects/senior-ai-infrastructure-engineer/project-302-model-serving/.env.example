# Serving Configuration
HOST=0.0.0.0
PORT=8000
WORKERS=4

# Model Configuration
TENSORRT_MODEL_PATH=/models/tensorrt/resnet50.engine
VLLM_MODEL_PATH=/models/llm/gpt2
VLLM_TENSOR_PARALLEL_SIZE=2

# Tracing Configuration
JAEGER_AGENT_HOST=jaeger
JAEGER_AGENT_PORT=6831
ENABLE_TRACING=true

# Monitoring
PROMETHEUS_PORT=8001
METRICS_PREFIX=model_serving

# Routing Configuration
ENABLE_AB_TESTING=true
ENABLE_CANARY=false
CANARY_PERCENTAGE=10

# Performance
MAX_BATCH_SIZE=32
MAX_WAIT_TIME_MS=50
GPU_MEMORY_UTILIZATION=0.9
