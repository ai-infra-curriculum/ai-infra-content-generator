{
  "dashboard": {
    "title": "Model Serving Performance Dashboard",
    "panels": [
      {
        "id": 1,
        "title": "Request Latency (P50, P95, P99)",
        "targets": [
          {"expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))"},
          {"expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))"},
          {"expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m]))"}
        ]
      },
      {
        "id": 2,
        "title": "Requests per Second",
        "targets": [{"expr": "rate(http_requests_total[5m])"}]
      },
      {
        "id": 3,
        "title": "Error Rate",
        "targets": [{"expr": "rate(http_requests_total{status=~\"5..\"}[5m])"}]
      },
      {
        "id": 4,
        "title": "Model Inference Time",
        "targets": [{"expr": "rate(model_inference_duration_seconds_sum[5m]) / rate(model_inference_duration_seconds_count[5m])"}]
      },
      {
        "id": 5,
        "title": "GPU Utilization",
        "targets": [{"expr": "DCGM_FI_DEV_GPU_UTIL"}]
      },
      {
        "id": 6,
        "title": "Active Model Instances",
        "targets": [{"expr": "model_instances_loaded"}]
      }
    ]
  },
  "description": "TODO for students: Customize panels for your model serving metrics, add alerting thresholds, create dashboard variables"
}